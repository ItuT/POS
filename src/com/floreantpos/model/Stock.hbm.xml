<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.floreantpos.model">
  <class lazy="false" name="com.floreantpos.model.Stock"  table="STOCK">
    <id column="ID" name="id" type="java.lang.Integer">
      <generator class="identity"/>
    </id>
    
    <property column="NAME" length="20" name="name" not-null="false" type="java.lang.String"/>
    <property column="COUNT" length="52" name="count" type="java.lang.Integer"/>
    <!-- Associations -->
  </class>
</hibernate-mapping>


