<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.floreantpos.model">
  <class lazy="false" name="com.floreantpos.model.PackagingUnit" table="PACKAGING_UNIT">
    <id column="ID" name="id" type="java.lang.Integer">
      <generator class="identity"/>
    </id>
    <property column="UNIT_NAME" name="unitName" type="java.lang.String" length="30"/>
    <property column="FACTOR" name="factor" type="java.lang.Double"/>
   	<property column="UNIT_CLASS" name="unitClass" type="java.lang.String" length="30"/>
  </class>
</hibernate-mapping>
